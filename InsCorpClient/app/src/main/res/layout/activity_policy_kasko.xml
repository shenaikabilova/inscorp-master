<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:layout_alignParentEnd="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:text="Собственик"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="11dp"
            android:id="@+id/etOwner"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etVehicleOwnerName"
            android:hint="Три имена" />

        <TextView
            android:text="ЕГН"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:id="@+id/textView3"
            android:layout_below="@+id/etOwner"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etEGN"
            android:layout_below="@+id/textView3"
            android:layout_alignStart="@+id/editText"
            android:hint="ЕГН" />

        <TextView
            android:text="Адрес"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:id="@+id/textView4"
            android:layout_below="@+id/textView6"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etAddress"
            android:hint="Адрес" />

        <TextView
            android:text="Телефон"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:id="@+id/textView5"
            android:layout_below="@+id/textView4"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="phone"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etPhoneNumber"
            android:hint="Телефонен №" />

        <TextView
            android:text="Регистрационен №"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="11dp"
            android:id="@+id/tvVehicleRegNum"
            android:layout_below="@+id/textView5"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etRegNum"
            android:hint="Регистрационен №" />

        <TextView
            android:text="Държава"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            android:layout_below="@+id/textView3"
            android:layout_marginTop="12dp" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etCountry"
            android:hint="Държава" />

        <TextView
            android:text="Тип МПС"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="17dp"
            android:id="@+id/tvVehicleType"
            android:layout_below="@+id/tvVehicleRegNum" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etType"
            android:hint="Тип МПС" />

        <TextView
            android:text="Подтип"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="18dp"
            android:id="@+id/tvVehicleSubtype"
            android:layout_below="@+id/tvVehicleType"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etSubtype"
            android:hint="Подтип" />

        <TextView
            android:text="Марка"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:id="@+id/tvVehicleBrand"
            android:layout_below="@+id/tvVehicleSubtype"
            android:layout_alignEnd="@+id/textView5" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etBrand"
            android:hint="Марка" />

        <TextView
            android:text="Модел"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:id="@+id/tvVehicleModel"
            android:layout_below="@+id/tvVehicleBrand"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etModel"
            android:hint="Модел" />

        <TextView
            android:text="РАМА"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:id="@+id/tvVehicleRAMA"
            android:layout_below="@+id/tvVehicleModel"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etRAMA"
            android:hint="РАМА" />

        <TextView
            android:text="Двигател"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:id="@+id/tvVehicleEngine"
            android:layout_below="@+id/tvVehicleRAMA"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etEngine"
            android:hint="Двигател" />

        <TextView
            android:text="Дата от"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:id="@+id/textView15"
            android:layout_below="@+id/textView14"
            android:layout_alignEnd="@+id/textView6" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etDateFrom"
            android:hint="Дата от" />

        <TextView
            android:text="Дата до"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView16"
            android:layout_below="@+id/textView15"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etDateTo"
            android:hint="Дата до" />

        <TextView
            android:text="Каско тип"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView7" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etKaskoType"
            android:hint="Каско тип" />

        <TextView
            android:text="Процент"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView9" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etPercent"
            android:hint="Процент" />

        <TextView
            android:text="Оценка"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/textView8" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etVehValue"
            android:hint="Оценка" />

        <TextView
            android:text="Сума"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView17"
            android:layout_alignParentBottom="true"
            android:layout_alignEnd="@+id/tvVehicleRAMA" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etValue"
            android:hint="Сума" />

    </LinearLayout>
</ScrollView>



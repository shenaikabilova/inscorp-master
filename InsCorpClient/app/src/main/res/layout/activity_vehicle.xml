<?xml version="1.0" encoding="utf-8"?>
<!--&lt;!&ndash;<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"&ndash;&gt;-->
    <!--&lt;!&ndash;xmlns:tools="http://schemas.android.com/tools"&ndash;&gt;-->
    <!--&lt;!&ndash;android:id="@+id/activity_vehicle"&ndash;&gt;-->
    <!--&lt;!&ndash;android:layout_width="match_parent"&ndash;&gt;-->
    <!--&lt;!&ndash;android:layout_height="match_parent"&ndash;&gt;-->
    <!--&lt;!&ndash;android:paddingBottom="@dimen/activity_vertical_margin"&ndash;&gt;-->
    <!--&lt;!&ndash;android:paddingLeft="@dimen/activity_horizontal_margin"&ndash;&gt;-->
    <!--&lt;!&ndash;android:paddingRight="@dimen/activity_horizontal_margin"&ndash;&gt;-->
    <!--&lt;!&ndash;android:paddingTop="@dimen/activity_vertical_margin"&ndash;&gt;-->
    <!--tools:context="com.kabilova.inscorpclient.VehicleActivity">-->
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:layout_alignParentEnd="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:text="Информация за МПС"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tvVehicleInfo" />

        <TextView
            android:text="Собственик"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="11dp"
            android:id="@+id/etOwner"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etVehicleOwnerName"
            android:hint="Три имена" />

        <TextView
            android:text="Регистрационен №"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="11dp"
            android:id="@+id/tvVehicleRegNum"
            android:layout_below="@+id/textView5"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etVehicleRegNum"
            android:hint="Регистрационен №" />

        <TextView
            android:text="Тип МПС"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="17dp"
            android:id="@+id/tvVehicleType"
            android:layout_below="@+id/tvVehicleRegNum" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etVehicleType"
            android:hint="Тип МПС" />

        <TextView
            android:text="Подтип"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="18dp"
            android:id="@+id/tvVehicleSubtype"
            android:layout_below="@+id/tvVehicleType"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etVehicleSubtype"
            android:hint="Подтип" />

        <TextView
            android:text="Марка"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:id="@+id/tvVehicleBrand"
            android:layout_below="@+id/tvVehicleSubtype"
            android:layout_alignEnd="@+id/textView5" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etVehicleBrand"
            android:hint="Марка" />

        <TextView
            android:text="Модел"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:id="@+id/tvVehicleModel"
            android:layout_below="@+id/tvVehicleBrand"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etVehicleModel"
            android:hint="Модел" />

        <TextView
            android:text="РАМА"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:id="@+id/tvVehicleRAMA"
            android:layout_below="@+id/tvVehicleModel"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etVehicleRAMA"
            android:hint="РАМА" />

        <TextView
            android:text="Двигател"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:id="@+id/tvVehicleEngine"
            android:layout_below="@+id/tvVehicleRAMA"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:focusable="false"
            android:ems="10"
            android:id="@+id/etVehicleEngine"
            android:hint="Двигател" />

        <TextView
            android:text="TextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tvVehicle" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etVehicleFirstReg"
            android:hint="Първа регистрация" />

        <TextView
            android:text="Години"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tvVehicleYears" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etVehicleYears"
            android:hint="Години" />

        <TextView
            android:text="Цвят"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tvVehicleColor" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/etVehicleColor"
            android:hint="Цвят" />

        <TextView
            android:text="Брой места"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tvVehiclePlaceNumber" />

        <EditText
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/etVehiclePlaceNumber"
            android:hint="Брой места" />

    </LinearLayout>
</ScrollView>

<!--</RelativeLayout>-->
